{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build IL2Workshop",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/IL2Workshop/IL2Workshop.csproj"
            ],
            "problemMatcher": "$tsc",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build Test",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Test/Test.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "build WorkshopStub",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/WorkshopStub/WorkshopStub.csproj"
            ],
            "problemMatcher": "$tsc"
        }
    ]
}